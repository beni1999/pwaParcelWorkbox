n*{\r\n    padding: 0;\r\n    margin: 0;\r\n    box-sizing: border-box;\r\n    list-style: none;\r\n    text-decoration: none;\r\n}\r\n\r\nbody{\r\n    display: flex;\r\n    min-height: 100vh;\r\n    flex-direction: column;\r\n    font-family: 'Montserrat';\r\n    background: linear-gradient(-45deg, #63c660 0%, #a6fd8c 100%);\r\n}\r\n\r\nmain {\r\n    flex: 1 0 auto;\r\n}\r\n\r\n.set{\r\n    position: fixed;\r\n    z-index:2;\r\n    bottom: 0;\r\n    right: 0;\r\n    display: flex;\r\n    justify-content: center;\r\n    left: 0;\r\n}\r\n\r\n.mobile{\r\n    background-color: #08c792;\r\n    color:#fff;\r\n    width: 100%;\r\n    height: 4.4rem;\r\n    border-radius: 5px;\r\n    text-align: center;\r\n    overflow: hidden;\r\n    box-shadow: 0 0 10px #bbe6e4;\r\n}\r\n\r\n.mobile ul{\r\n    justify-content: space-evenly;\r\n    display: flex;\r\n}\r\n\r\nul#view-mobile{\r\n    height: 100%;\r\n}\r\n\r\n.mobile ul .nav-item{\r\n    cursor: pointer;\r\n    text-transform:capitalize;\r\n    transition: .2s linear;\r\n}\r\n\r\n.mobile ul .nav-item:hover{\r\n    background-color: #084b83;\r\n}\r\n\r\n.mobile ul .nav-item a{\r\n    color: #fff;\r\n    transition: .2s linear;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    height: 100%;\r\n    padding: 0 1rem;\r\n}\r\n\r\n.mobile ul .nav-item i{\r\n    font-size: 2rem;\r\n    transition: .2s linear;\r\n}\r\n\r\n#card:hover{\r\n    opacity:.8;\r\n    transform: scale(1.076);\r\n    cursor: pointer;\r\n}\r\n\r\n#logoContainer{\r\n    background: linear-gradient(to bottom, #ffe838, #fd57bf);\r\n    -webkit-background-clip: text;\r\n    -webkit-text-fill-color: transparent;\r\n    font-weight: bold;\r\n}\r\n\r\n/* footer */\r\n/* -------------------------------------------- */\r\n#footer{\r\n    border-top: 2px solid #016e40;\r\n    width: 100%;\r\n    text-align: center;\r\n    background-color: #08C792;\r\n    padding-bottom: 1rem;\r\n    position: relative;\r\n    bottom: 0;\r\n    box-shadow: 1px 1px 5px black;\r\n    transition:margin-left 1s;\r\n  }\r\n\r\n  .social-icon {\r\n    margin: 15px 10px;\r\n    color: blue;\r\n  }\r\n\r\n  #footer p {\r\n    font-size: 1rem;\r\n    color: black;\r\n  }\r\n\r\n  #footer>a {\r\n    text-decoration: none;\r\n  }\r\n\r\n\r\n/* mobile */\r\n@media (max-width:500px){\r\n    #footer{\r\n        display: none;\r\n    }\r\n    .mobile {\r\n        width: 100vw;\r\n        height: 4rem;\r\n    }\r\n    .mobile ul .nav-item i {\r\n        font-size: 1.3rem;\r\n        margin-bottom: .4rem;\r\n    }\r\n    .mobile ul .nav-item a {\r\n        font-size: .8rem;\r\n        padding: 0 .6rem;\r\n    }\r\n    #logoContainer{\r\n        font-size: 1.4rem;\r\n    }\r\n    #logoContainer img{\r\n        height: 1.4rem;\r\n        transform: translateY(.1rem);\r\n    }\r\n}\r\n/*tablet */\r\n@media (min-width:500px) and (max-width:768px){\r\n    .mobile {\r\n        width: 80vw;\r\n    }\r\n    #footer{\r\n        display: none;\r\n    }\r\n\r\n    .mobile ul .nav-item i {\r\n        font-size: 1rem;\r\n    }\r\n}\r\n/* desktop*/\r\n@media (min-width:768px){\r\n    .mobile {\r\n        width: 70vw;\r\n    }\r\n\r\n    #footer{\r\n        display: none;\r\n    }\r\n    .mobile ul .nav-item i {\r\n        font-size: 1.8rem;\r\n    }\r\n\r\n\r\n}\r\n\r\n/* desktop*/\r\n@media (min-width:992px){\r\n    .set {\r\n        display: none;\r\n    }\r\n    #footer{\r\n        display: block;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"ede45bbcfb661c56e9541791b72f9961","cacheData":{"env":{}}}