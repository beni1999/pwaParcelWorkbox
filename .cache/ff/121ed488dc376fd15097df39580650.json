d":{"line":1,"column":2370}},{"source":"scripts/component/team.js","name":"data","original":{"line":23,"column":16},"generated":{"line":1,"column":2379}},{"source":"scripts/component/team.js","name":"showTeams","original":{"line":24,"column":12},"generated":{"line":1,"column":2382}},{"source":"scripts/component/team.js","name":"data","original":{"line":24,"column":22},"generated":{"line":1,"column":2384}},{"source":"scripts/component/team.js","name":"catch","original":{"line":26,"column":11},"generated":{"line":1,"column":2389}},{"source":"scripts/component/team.js","original":{"line":26,"column":17},"generated":{"line":1,"column":2395}},{"source":"scripts/component/team.js","name":"error","original":{"line":26,"column":17},"generated":{"line":1,"column":2404}},{"source":"scripts/component/team.js","name":"console","original":{"line":27,"column":12},"generated":{"line":1,"column":2407}},{"source":"scripts/component/team.js","name":"log","original":{"line":27,"column":20},"generated":{"line":1,"column":2415}},{"source":"scripts/component/team.js","name":"error","original":{"line":27,"column":24},"generated":{"line":1,"column":2419}}],"sources":{"scripts/component/team.js":"import {fetchData, Api} from '../source/data-source.js';\r\nimport {showLoader, hideLoader} from './preloader.js';\r\nimport dbPromise from './db.js';\r\n\r\nlet teamsData = null;\r\nconst Content = document.getElementById('Content');\r\nconst title = document.getElementById('headerTitle');\r\n\r\nexport default function teams(){\r\n    const loadTeams = () => {\r\n        showLoader();\r\n        if (\"caches\" in window) {\r\n          caches.match(Api.Teams).then(response=>{\r\n            if (response) {\r\n              response.json().then(data=> {\r\n                showTeams(data);\r\n              });\r\n            }\r\n          });\r\n        }\r\n\r\n        fetchData(Api.Teams)\r\n          .then(data => {\r\n            showTeams(data);\r\n          })\r\n          .catch(error => {\r\n            console.log(error);\r\n          });\r\n\r\n      };\r\n\r\n    function showTeams(data) {\r\n        let content = \"\";\r\n        let renderTarget = Content;\r\n        teamsData=data;\r\n\r\n        dbPromise.then(function(db){\r\n          let tx = db.transaction(\"teams\", \"readwrite\").objectStore(\"teams\");\r\n          const txFTeams = tx.getAll() || [];\r\n          return txFTeams;\r\n        })\r\n        .then(team=>{\r\n          showLoader();\r\n            const favTeams = team.map(i => i.id);\r\n            function renderButton(teamId) {\r\n              if (!favTeams.includes(teamId)) {\r\n                  return `\r\n                    <a class=\"btn-floating btn-medium halfway-fab waves-effect waves-light green addFavorite\"\r\n                    >\r\n                      <i id=\"icon-${teamId}\" class=\"large material-icons\" data-id=\"${teamId}\">favorite</i>\r\n                    </a>`;\r\n              } else {\r\n                  return \"\";\r\n            }\r\n          };\r\n\r\n          data.teams.forEach(team=> {\r\n            content += `\r\n              <div class=\"col s12 m6 l4\">\r\n                  <div class=\"card\" id=\"card\" style=\"height:21rem;\">\r\n                      <div class=\"card-image\">\r\n                          <img src=\"${\r\n                            team.crestUrl\r\n                          }\" style=\"padding: 16px; margin: auto; width: 6rem; \">\r\n                          ${renderButton(team.id)}\r\n                      </div>\r\n                      <div class=\"card-content\">\r\n                          <h6>${team.name}</h6>\r\n                          <p>Founded: ${team.founded}</p>\r\n                          <p>Venue: ${team.venue}</p>\r\n                          <a href=\"${team.website}\">${team.website}</a>\r\n                      </div>\r\n                  </div>\r\n              </div>\r\n              `;\r\n              title.innerHTML = \"Seri A Italy Teams\";\r\n              renderTarget.innerHTML = content;\r\n              hideLoader();\r\n              const addFavorite= document.querySelectorAll('.addFavorite');\r\n\r\n              addFavorite.forEach(el=>{\r\n                el.addEventListener('click',e=>{\r\n                  let getId = e.target.dataset.id;\r\n                  addFavoriteTeam(getId);\r\n                })\r\n              })\r\n          })\r\n        })\r\n\r\n      };\r\n\r\n      function addFavoriteTeam(teamId) {\r\n        let teamObject = teamsData.teams.filter(el => el.id == teamId)[0];\r\n        dbPromise.then(db=>{\r\n          const tx = db.transaction(\"teams\", \"readwrite\");\r\n          const store = tx.objectStore(\"teams\");\r\n          store.add(teamObject);\r\n          return tx.complete;\r\n        })\r\n        .then(()=>{\r\n          // remove the button\r\n          let element = document.getElementById(`icon-${teamId}`);\r\n          element.parentNode.remove();\r\n          swal({\r\n            title: \"succes!\",\r\n            text: `${teamObject.name} ditambahkan ke Team Favorit.`,\r\n            icon: \"success\",\r\n            button: \"Ok!\",\r\n          });\r\n        })\r\n\r\n    };\r\n\r\n    loadTeams();\r\n};"},"lineCount":null}},"error":null,"hash":"ab749f0083df416542e1a70d6df71725","cacheData":{"env":{}}}